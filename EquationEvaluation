import java.util.Scanner;
public class EquationEvaluation {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		double k;
		double A;
		double T1;
		double T2;
		double L;
		
		System.out.println("Enter the value of k");
		k = input.nextDouble();
		System.out.println("Enter the value of A");
		A = input.nextDouble();
		System.out.println("Enter the value of T1");
		T1 = input.nextDouble();
		System.out.println("Enter the value of T2");
		T2 = input.nextDouble();
		System.out.println("Enter the value of L");
		L = input.nextDouble();

		System.out.printf("The result is: %f ", calc(k, A, T1, T2, L));
		
	}//end class

	public static double calc(double k, double A, double T1, double T2, double L) {
	
		double result = (k * A * (T1-T2) /L);
		
		return result;
	}//end class
}//end main
