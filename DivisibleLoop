public class DivisibleLoop {

	public static void main(String[] args) {
        for (int num = 100; num <= 1000; num = num + 1) {

            if (num % 5 == 0 && num % 6 == 0)
                System.out.print((num % 10 != 0) ? num + " " : num + "\n");
        } //end loop
    } //end class
} //end main
